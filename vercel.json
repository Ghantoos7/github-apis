{
    "version": 2,
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "main.py"
      }
    ],
    "env": {
      "AUTH_KEY": "@auth_key"
    },
    "build": {
      "env": {
        "AUTH_KEY": "@auth_key"
      }
    }
  }
  